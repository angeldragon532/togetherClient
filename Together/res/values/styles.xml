     <!-- 简单标题 -->
    <style name="title_text_mid">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:gravity">center</item>
        <item name="android:textSize">@dimen/title_text_mid</item>
        <item name="android:textColor">@color/white</item>
    </style>
    
    <style name="main_menu_text">
        <item name="android:textSize">18sp</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textColor">@color/black</item>
    </style>
    
    <style name="main_menu_image">
        <item name="android:layout_width">50dp</item>
        <item name="android:layout_height">50dp</item>
    </style>
    
    <!-- project style -->
     <style name="project_status_text">
        <item name="android:textColor">@color/black</item>
        <item name="android:textSize">12sp</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
    </style>
    
     <style name="project_status_image">
        <item name="android:layout_width">20dp</item>
        <item name="android:layout_height">20dp</item>
    </style>
    
      <style name="project_condition_dialog_text">
        <item name="android:textColor">@color/project_list_conditiondialog_title_color</item>
        <item name="android:textSize">22sp</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:gravity">center</item>
    </style>
    
      <style name="project_condition_dialog_value">
        <item name="android:textColor">@color/project_list_conditiondialog_subtitle_color</item>
        <item name="android:textSize">16sp</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
    </style>
    
      <style name="project_serach_promote_text">
        <item name="android:textColor">@color/project_search_promote_color</item>
        <item name="android:textSize">16sp</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
    </style>
  
      
    <!-- 用户数据生成 -->
    <style name="user_info_init_text">
	    <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textSize">18dp</item>
        <item name="android:textColor">@color/black</item>
	</style> 
	
    <style name="user_info_init_value">
	    <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textSize">18dp</item>
        <item name="android:textColor">@color/common_hint_grey</item>
	</style> 

    <style name="LaunchActivityTheme" parent="@android:style/Theme">
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowBackground">@android:color/white</item>
        <item name="android:windowIsTranslucent">true</item>
    </style>
    
   <!-- checkbox 样式 -->
<style name="CustomCheckboxTheme" parent="@android:style/Widget.CompoundButton.CheckBox">  
   <item name="android:button">@drawable/checkbox_style</item>  
   <item name="android:layout_width">20dp</item>   <item name="android:layout_height">20dp</item> 	
</style> 

<style name="ProjectCheckboxTheme" parent="@android:style/Widget.CompoundButton.CheckBox">  
   <item name="android:button">@drawable/projectcondition_checkbox_style</item>
   <item name="android:paddingRight">10dp</item> 
   <item name="android:layout_width">wrap_content</item> 
   <item name="android:layout_height">wrap_content</item> 
</style>

<style name="common_checkbox" parent="@android:style/Widget.CompoundButton.CheckBox">  
   <item name="android:paddingRight">5dp</item> 
   <item name="android:layout_width">wrap_content</item>
   <item name="android:layout_height">wrap_content</item>
   <item name="android:textSize">18dp</item>
   <item name="android:textColor">@color/black</item>
</style>
    

