    <!-- text style -->
    <style name="common_text_white">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textColor">@color/common_white</item>
        <item name="android:textSize">@dimen/common_nomal_text</item>
         <item name="android:gravity">center</item>
    </style>

    <style name="common_text_whitebold">
        <item name="android:textColor">@color/common_white</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textSize">16sp</item>
        <item name="android:textStyle">bold</item>
    </style>
    
     <style name="common_text_hint_grey">
        <item name="android:textColor">@color/common_hint_grey</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textSize">16sp</item>
    </style>
    
    <style name="common_text_title_black">
        <item name="android:textColor">@color/common_black</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textSize">18sp</item>
        <item name="android:textStyle">bold</item>
    </style>
    
    <style name="common_text_title_black_small_size">
        <item name="android:textColor">@color/common_black</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textSize">14sp</item>
        <item name="android:singleLine">true</item>
        <item name="android:ellipsize">end</item>
    </style>
    
    
    <!-- 普通文字加粗的黑色字体名称 -->
    <style name="common_text_bold_black_mid_text">
        <item name="android:textColor">@color/common_black</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textSize">16sp</item>
        <item name="android:textStyle">bold</item>
    </style>
    
    <!-- 普通文字加粗的黑色字体名称 -->
    <style name="common_text_bold_black_mid_value">
        <item name="android:textColor">@color/common_gray</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textSize">16sp</item>
        <item name="android:textStyle">bold</item>
    </style>

   
